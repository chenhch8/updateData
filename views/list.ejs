<div>
    <% if (date) { %>
    <form action="/download" method="get">
        <button type="submit" class="btn btn-success pull-right "><span class="glyphicon glyphicon-save"></span>导出表格</button>
    </form>
    <% } %>
    <table class="table table-striped table-bordered table-hover table-condensed">
        <thead>
        <tr>
            <th>序号</th>
            <th>校区</th>
            <th>学院</th>
            <th>类型</th>
            <th>国籍</th>
            <th>身份证/护照</th>
            <th>学号/工号</th>
            <th>拼音/外文姓名(FullName)</th>
            <th>拼音/外文姓(sName)</th>
            <th>中文姓名</th>
            <th>中文姓(sName_zh)</th>
            <th>中文名(gName_zh)</th>
            <th>备注/电话</th>
            <th>提交时间</th>
        </tr>
        </thead>
        <tbody>
        <% list.forEach(function (user, index) { %>
        <tr>
            <td><%= index %></td>
            <td><%= user.campus %></td>
            <td><%= user.academy %></td>
            <td><%= user.plptype %></td>
            <td><%= user.country %></td>
            <td><%= user.idnumber %></td>
            <td><%= user.stdnum %></td>
            <td><%= user.fullname %></td>
            <td><%= user.sname %></td>
            <td><%= user.name_zh %></td>
            <td><%= user.sname_zh %></td>
            <td><%= user.gname_zh %></td>
            <td><%= user.remark %></td>
            <td><%= user.updateDate %></td>
        </tr>
        <% }) %>
        </tbody>
    </table>
</div>

<script src="../javascripts/detail.js" rel="type/javascript"></script>

<%- include footer %>